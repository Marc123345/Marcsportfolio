{
  "name": "marc-friedman-portfolio",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "build": "npx vite build && node generate-sitemap.mjs",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "spellcheck": "eslint . --ext ts,tsx,js,jsx --rule 'spellcheck/spell-checker: 1'",
    "sitemap": "node generate-sitemap.mjs",
    "check:indexing": "node scripts/check-indexing-issues.js",
    "seo:check": "node scripts/check-indexing-issues.js && npm run sitemap",
    "cloudflare:get": "node scripts/cloudflare-transformations.js get",
    "cloudflare:enable-all": "node scripts/cloudflare-transformations.js enable-all"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@plausible-analytics/tracker": "^0.4.4",
    "@supabase/supabase-js": "^2.46.1",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-query": "^5.59.16",
    "framer-motion": "^11.11.17",
    "gsap": "^3.12.5",
    "lucide-react": "^0.454.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.67.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.53.2",
    "react-router-dom": "^6.28.0",
    "sitemap": "^7.1.1",
    "sonner": "^1.7.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-spellcheck": "^0.0.20",
    "globals": "^15.12.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.14",
    "terser": "^5.36.0",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.13.0",
    "vite": "7.0.6",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
